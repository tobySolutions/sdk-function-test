{
  "name": "@fleek-platform/sdk",
  "description": "Fleek Platform SDK",
  "version": "3.4.0",
  "main": "./dist/browser/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/browser/index.js"
    },
    "./node": {
      "types": "./dist/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./browser": {
      "types": "./dist/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "dependencies": {
    "@genql/cli": "^6.3.3",
    "@noble/hashes": "^1.4.0",
    "@web-std/file": "^3.0.2",
    "@web-std/form-data": "^3.0.2",
    "axios": "^1.7.2",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.0.3",
    "files-from-path": "^1.0.0",
    "graphql": "^16.9.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "object-traversal": "^1.0.1",
    "parallel-transform-web": "^1.0.0"
  },
  "devDependencies": {
    "@fleek-platform/sites": "^1.5.24",
    "@jspm/core": "^2.0.1",
    "@types/detect-node": "^2.0.0",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@types/whatwg-fetch": "^0.0.33",
    "@vitest/coverage-v8": "1.3.1",
    "@web3-storage/upload-client": "^17.0.1",
    "bun": "^1.1.20",
    "concurrently": "^7.6.0",
    "decimal.js": "^10.4.3",
    "esbuild": "^0.21.4",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "esbuild-plugins-node-modules-polyfill": "^1.6.4",
    "import-meta-resolve": "^4.1.0",
    "ipfs-http-client": "56.0.3",
    "multiformats": "^12.1.3",
    "native-fetch": "^4.0.2",
    "typescript": "5.5.3",
    "vitest": "1.3.1",
    "@fleek-platform/auth": "2.0.11",
    "@fleek-platform/env-guards": "1.8.1",
    "@fleek-platform/errors": "2.5.1",
    "@fleek-platform/gql-client-utils": "1.4.48",
    "@fleek-platform/graphql": "2.21.4",
    "@fleek-platform/tester": "2.9.0",
    "eslint-config-fleek-custom": "1.0.0"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "clean": "pnpm \"/^clean:.*/\"",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "build": "pnpm clean:dist && doppler run --command='pnpm transpile && bun run ./build.ts'",
    "build:ci": "pnpm run build",
    "build:types": "tsc -p tsconfig.types.json",
    "watch": "tsc --watch",
    "lint": "eslint --fix ./build.ts --ext .js,.jsx,.ts,.tsx src",
    "transpile": "pnpm exec tsc -p tsconfig.json",
    "tsc": "tsc --noEmit",
    "test:disabled": "vitest run --dir src",
    "increaseVersion": "pnpm version prerelease --preid dev",
    "release": "pnpm publish --access=public --no-git-checks"
  }
}